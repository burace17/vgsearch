@model IEnumerable<vgsearch.Models.Developer>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.city_founded)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.country_founded)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.city_founded)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.country_founded)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.developer_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.developer_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.developer_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
